&mdss_dsi0 {
	status = "okay";
	vdda-supply = <&vdda_mipi_dsi0_1p2>;

	display_panel: panel@0 {
		reg = <0>;

		reset-gpios = <&tlmm 6 GPIO_ACTIVE_LOW>;

		vddio-supply = <&vreg_l14a_1p8>;

		panel-te-gpios = <&tlmm 8 GPIO_ACTIVE_LOW>;
		pinctrl-names = "panel_active", "panel_suspend", "default";
    		pinctrl-0 = <&sde_dsi_active &sde_te_active &display_panel_avdd_eldo_default>;
        	pinctrl-1 = <&sde_dsi_suspend &sde_te_suspend &display_panel_avdd_eldo_off>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mdss_dsi0_out>;
			};
		};
	};
};

&mdss_dsi0_out {
	data-lanes = <0 1 2 3>;
	remote-endpoint = <&panel_in>;
};

&mdss_dsi0_phy {
	status = "okay";
	vdds-supply = <&vdda_dsi_0_pll_0p9>;
};

&mdss_dp {
	status = "okay";
};

&mdss_dp_out {
	data-lanes = <0 1>;
	remote-endpoint = <&usb_1_qmpphy_dp_in>;
};

&mdss {
	status = "okay";
};

&pm8150b_vbus {
	status = "okay";
};

&gmu {
